# Ignore directories generated by Composer
/bin/
/console/language/
/drush/Commands/
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.local.php

# Ignore services configuration
/web/sites/*/services.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env

# Ignore the vagrant files ( except config/config.yml)
/.vagrant/
Vagrantfile

# Ignore build directories
build

.editorconfig
.gitattributes

# Ignore local drush configuration.
/drush/drush.yml
/.drush-lock-update

# Ignore configuration for Senator created webforms
/config/sync/webform.webform.*

# Don't ignore these sitewide webforms
!/config/sync/webform.webform.school_form_earth_day.yml
!/config/sync/webform.webform.school_form.yml
!/config/sync/webform.webform.school_form_thanksgiving.yml
!/config/sync/webform.webform.senator_contact_form.yml
!/config/sync/webform.webform.contact.yml
!/config/sync/webform.webform.youth_leadership.yml
!/config/sync/webform.webform.general_inquiry.yml

# Ignore global config settings
/config/sync/nys_config.settings.yml
