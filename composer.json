{
    "name": "mediacurrent/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": ["drupal/views_exclude_previous"]
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/views_exclude_previous-3128475.git"
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.7.0",
        "drupal/address_autocomplete": "^1.0@beta",
        "drupal/address_map_link": "^1.3",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/charts": "^3.3",
        "drupal/clamav": "^2.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "^9.0",
        "drupal/core-recommended": "^9.0@stable",
        "drupal/diff": "^1.0",
        "drupal/eck": "^1.0@beta",
        "drupal/email_registration": "^1.1",
        "drupal/entityqueue": "^1.2",
        "drupal/eva": "^2.1",
        "drupal/facets": "^2.0",
        "drupal/fancy_file_delete": "^2.0",
        "drupal/field_permissions": "^1.1",
        "drupal/field_validation": "^1.0@beta",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/flag": "^4.0@beta",
        "drupal/flood_control": "^2.2",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/geocoder": "^3.25",
        "drupal/geolocation": "^3.7",
        "drupal/google_tag": "^1.4",
        "drupal/guardr_core": "^2.0",
        "drupal/honeypot": "^2.0",
        "drupal/htmlpurifier": "^1.0@RC",
        "drupal/image_style_quality": "^1.4",
        "drupal/imagemagick": "^3.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/job_scheduler": "^3.1",
        "drupal/location_migration": "^1.0@beta",
        "drupal/login_history": "^1.1",
        "drupal/mailsystem": "^4.3",
        "drupal/migrate_devel": "^2.0@alpha",
        "drupal/migrate_plus": "^5.2",
        "drupal/migrate_tools": "^5.1",
        "drupal/name": "^1.0@RC",
        "drupal/node_export": "^1.0@alpha",
        "drupal/oembed_providers": "^2.0",
        "drupal/password_policy": "^3.1",
        "drupal/permissions_filter": "^1.2",
        "drupal/recaptcha": "^3.0",
        "drupal/redis": "^1.5",
        "drupal/reroute_email": "^2.1",
        "drupal/search_api_solr": "^4.2",
        "drupal/seckit": "^2.0",
        "drupal/semanticviews": "^2.3",
        "drupal/site_verify": "1.x-dev",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/twitter_block": "^3.0@alpha",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_data_export": "^1.1",
        "drupal/views_exclude_previous": "dev-3128475-drupal-9-readiness",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/views_list_sort": "^1.4",
        "drupal/views_send": "^1.6",
        "drupal/votingapi": "^3.0@beta",
        "drupal/webform": "^6.1",
        "drush/drush": "^10.3",
        "geocoder-php/google-maps-provider": "^4.6",
        "kint-php/kint": "^4.1",
        "mediacurrent/mis_profile": "^2.0",
        "mediacurrent/rain": "^3.23",
        "mediacurrent/rain_admin": "^2.0@beta",
        "mediacurrent/rain_demo": "^2.0@beta",
        "mediacurrent/rain_features": "^3.0@beta",
        "mediacurrent/rain_theme": "^4.0@beta",
        "npm-asset/datatables.net-dt": "^3.2",
        "oomphinc/composer-installers-extender": "^2.0",
        "pantheon-systems/drupal-integrations": "^9.0",
        "politsin/jquery-ui-touch-punch": "^1.0",
        "sendgrid/php-http-client": "^3.10.6",
        "sendgrid/sendgrid": "^7.11",
        "webflo/drupal-finder": "^1.0.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9.0",
        "drupal/devel": "*",
        "drupal/drupal-extension": "^4.1",
        "drush-ops/behat-drush-endpoint": "^9.3.0",
        "mediacurrent/ci-scripts": "^1.11.0",
        "mediacurrent/ci-tests": "dev-master",
        "mglaman/drupal-check": "^1.0",
        "phpro/grumphp": "^1.0.0",
        "phpspec/prophecy-phpunit": "^2",
        "vlucas/phpdotenv": "^2.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "autoload-dev": {
        "files": ["load.environment.php"]
    },
    "scripts": {
        "robo": "$(pwd)/bin/robo --ansi --load-from $(pwd)/scripts",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches": {
            "drupal/core": {
                "2827921 - Exception thrown by responsive srcset images when the image is not yet in the file system (such as with Stage File Proxy)": "https://www.drupal.org/files/issues/2021-09-03/responsive_image-remove_missing_response_image_width_exception-2827921-22.patch",
                "2949017 - There is no way to delete or update file entities of other users": "https://www.drupal.org/files/issues/2021-12-01/2949017-85.patch",
                "763380 - Do not use \\Drupal\\taxonomy\\TermStorageInterface::loadTree() in \\Drupal\\taxonomy\\Form\\OverviewTerms::buildForm()": "https://www.drupal.org/files/issues/2021-05-26/763380-73.patch",
                "1671190 - Use <button /> form element type instead of <input type='submit'/>": "https://www.drupal.org/files/issues/2022-01-07/1671190-92.patch"
            },
            "drupal/email_registration": {
                "2843505 - Add support for Behat tests": "https://www.drupal.org/files/issues/2020-10-21/2843505-39.patch"
            },
            "drupal/htmlpurifier": {
                "2989756 - HTMLPurifier Serializer caches into vendor directory by default": "https://www.drupal.org/files/issues/2021-09-24/serializer-caches-into-vendor-d9-2989756-14.patch"
            },
            "drupal/inline_entity_form": {
                "3208818 - Migration Error: Cannot use string offset as an array in MigrationHelper.php on line 53": "https://www.drupal.org/files/issues/2021-12-23/3208818-7.patch",
                "3099844 - Required fields make an optional IEF (erroneously) required": "https://www.drupal.org/files/issues/2021-07-19/inline_entity_form-required_fields_optional_ief-3099844-24.patch"
            },
            "drupal/location_migration": {
                "#3192657-13: Data fix": "https://www.drupal.org/files/issues/2021-08-09/location_migration-source_count-3192657-13.patch",
                "#3214749-18: Map all formatters": "https://www.drupal.org/files/issues/2021-06-08/3214749-18.patch"
            },
            "drupal/votingapi": {
                "#3192657-2: Update drush command declaration with regard to options": "https://www.drupal.org/files/issues/2022-07-07/votingapi-3294809.patch"
            }
        }
    }
}
