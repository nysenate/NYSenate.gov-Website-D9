{#
/**
 * @file
 * Default simple view template to display a list of rows.
 *
 * @ingroup views_templates
 */
#}

{% if title %}
  <{{ group.element }}{{ group.attributes }}>
    {{ title }}
  </{{ group.element }}>
{% endif %}

{% if list.element %}
  <{{ list.element }}{{ list.attributes }}>
{% endif %}

{% for id, row in rows %}

  {% if row.element %}
  <{{ row.element }} {{ row.attributes }}>
  {% endif %}

    {{ row.content }}

  {% if row.element %}
  </{{ row.element }}>
  {% endif %}

{% endfor %}

{% if list.element %}
  </{{ list.element }}>
{% endif %}
