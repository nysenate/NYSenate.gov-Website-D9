<div class="c-block c-container c-container--want-to">
    <div class="c-container--header">
        <h2 class="c-container--title">I want to...</h2>
    </div>

    <div class="c-container--body">
        {{ senator_section }}

        <div class="c-want-to--item c-want-to--item__explore">
            <h3 class="c-want-to--cta">...find out more about issues that matter to me.</h3>
            <a href="/explore-issues" class="c-block--btn"><span class="btn-text">explore issues</span></a>
        </div>

        <div class="c-want-to--item c-want-to--item__news">
            <h3 class="c-want-to--cta">...keep up with Senate news</h3>
            <a href="/news-and-issues" class="c-block--btn"><span class="btn-text">see all news</span></a>
        </div>
    </div>
</div>
