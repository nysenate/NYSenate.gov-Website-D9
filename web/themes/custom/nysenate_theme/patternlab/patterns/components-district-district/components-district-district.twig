{{ attach_library('nysenate_theme/district') }}

<div class="district">
  <div class="district__header">
    <h2 class="nys-title nys-title--district">Our District</h2>
    <h3 class="c-subpage-header--subtitle05">{{ name }}</h3>
  </div>

  <div class="district__video">
    <div class="c-block">
      <iframe height="414" scrolling="no" src="{{ video }}" width="100%"></iframe>
    </div>
  </div>

  <div class="district__subheading c-subpage-header--title03">
    {{ subhead }}
  </div>

  <div class="district__body">
    <div class="c-block">
      {{ body|raw }}
    </div>
  </div>
</div>
