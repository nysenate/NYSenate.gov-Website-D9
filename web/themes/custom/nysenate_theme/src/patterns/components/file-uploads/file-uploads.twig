{{ attach_library('nysenate_theme/file-uploads') }}

<div class="c-block c-block--pg-file-uploads {{ classes }}">
  {{ title_prefix }}
  {{ title_suffix }}
  <h2 class="pg-subtitle">
    {{ title }}
  </h2>
  <div class="c-news--download">
    <span class='file'>
      <img class='file-icon' alt='PDF icon' title='application/pdf' src='/modules/file/icons/application-pdf.png'>
      {{ attachment }}
    <span>
  </div>
</div>
